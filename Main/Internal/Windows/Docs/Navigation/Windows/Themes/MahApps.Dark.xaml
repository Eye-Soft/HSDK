<ResourceDictionary 
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Color x:Key="BackgroundKey">#252525</Color>
	<Color x:Key="BackgroundDarkKey">#010101</Color>
	<Color x:Key="BorderKey">#454545</Color>

	<SolidColorBrush x:Key="BorderColor" Color="{DynamicResource BorderKey}"/>
	<SolidColorBrush x:Key="BackgroundDarkColor" Color="{DynamicResource BackgroundDarkKey}" />
	<SolidColorBrush x:Key="BackgroundColor" Color="{DynamicResource BackgroundKey}" />

	<Color x:Key="HighlightDarkColor">#FF086F9E</Color>
	<Color x:Key="HighlightLightColor">#FF086F9E</Color>
	<Color x:Key="AccentDarkColor">#FF1c739d</Color>
	<Color x:Key="AccentColor">#191919</Color>
	<Color x:Key="AccentColor2">#99119EDA</Color>
	<Color x:Key="AccentColor3">#66119EDA</Color>
	<Color x:Key="AccentColor4">#33119EDA</Color>

	<LinearGradientBrush x:Key="BrandingBrush" EndPoint="0.001,0.5" StartPoint="1.002,0.5">
		<GradientStop Color="{DynamicResource HighlightDarkColor}" Offset="0" />
		<GradientStop Color="#FF5FC316" Offset="1" />
	</LinearGradientBrush>
	<SolidColorBrush x:Key="AccentColorBrush" Color="{DynamicResource AccentColor}" />
	<SolidColorBrush x:Key="AccentSelectedColorBrush" Color="White" />
	<SolidColorBrush x:Key="HighlightLightColorBrush" Color="{DynamicResource HighlightLightColor}" />
</ResourceDictionary>